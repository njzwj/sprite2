doctype html
html    
    head
        meta(charset='utf-8')
        title statuslist
        link(href="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/css/bootstrap.min.css",rel="stylesheet")
     body
        div(class = "container")
            div(id = "row clearfix")
                div(id = "col-md-12 column")
                    nav(id = "navbar navbar-default navbar-inverse navbar-fixed-top",role = "navigation") 
                        div(class = "navbar-header")
                            button(type="button",class="navbar-toggle",data-toggle="collapse",data-target="#bs-example-navbar-collapse-1")
                                span(class="sr-only") Toggle navigation
                                span(class="icon-bar")
                                span(class="icon-bar")
                                span(class="icon-bar")
                            a(class="navbar-brand",href="#") Home
                        div(class="collapse navbar-collapse",id="bs-example-navbar-collapse-1")
                            ul(class="nav navbar-nav")
                                li(class="active")
                                    a(href="#") Problem
                                li(class="active")
                                    a(href="#") Status
                                li(class="active")
                                    a(href="#") Contest
                            form(class="navbar-form navbar-left",role="search")
                                div(class="form-group")
                                    input(type="text",class="form-control")
                                button(type="submit",class="btn btn-default") Submit
                            ul(class="nav navbar-nav navbar-right")
                                li(class="active")
                                    //-这个NAME还需要进一步设计
                                    a(href="#") Name 
                                li(class="active")
                                    //-这个Update更新到个人主页
                                    a(href="#") Update
                    div(class="jumbotron well")
                        p 
                            //-此处还需要改进
                            h3 ACM-ICPC World Final NWPU
                    ul(class="nav nav-tabs")
                        li(class="disabled")
                            a(href="#") Problem
                        li(class="disabled")
                            a(href="#") Rank
                        li(class="active")
                            a(href="#") Process
                        li(class="disabled")
                            a(href="disabled") Discuss/Ask
                    table(class="table")
                        thead
                            tr
                                th judgeID
                                th ProblemID
                                th Result
                                th team
                                th usedtime(ms)
                                th memory(kb)
                                th handtime
                        tbody

                            //-这个地方的具体颜色还需要调整
                            - for(var i=0;i<2;i++)
                                tr(class="success")
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td Accepted
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11

                            - for(var i=2;i<4;i++)
                                tr(class="danger")
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td Wrong Answer
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11 
                            - for(var i=4;i<6;i++)
                                tr(class="info")
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td pending
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11 
                            - for(var i=6;i<8;i++)
                                tr(class="warning")
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td TimeLimitExceed
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11 
                            - for(var i=8;i<10;i++)
                                tr(class="active")
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td Runtime Error
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11 
                            - for(var i=10;i<12;i++)
                                tr
                                    td 0000#{i}
                                    td 
                                        a(href="#") 00#{i}
                                    td MemoryLimitExceed
                                    td make_a_big_news
                                    td 22
                                    td 1000
                                    td 2016.11.11.11.11
                div(class="pull-left")
                ul(class="pagination")
                    li
                        a(href="#") &laquo;
                    -for(var i = 1;i<=5;i++) 
                        li
                            a(href="#") #{i}
                    li
                        a(href="#") ...
                    li
                        a(href="#") 10
                    li 
                        a(href="#") &raquo;
                                