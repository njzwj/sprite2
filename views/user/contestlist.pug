doctype html
html
    head
        meta(charset='utf-8')
        title contentlist 
        link(href="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/css/bootstrap.min.css",rel="stylesheet")
    .body
        div(class = "container")
            div(id = "row clearfix")
                div(id = "col-md-12 column")
                    nav(id = "navbar navbar-default navbar-inverse navbar-fixed-top",role = "navigation") 
                        div(class = "navbar-header")
                            button(type="button",class="navbar-toggle",data-toggle="collapse",data-target="#bs-example-navbar-collapse-1")
                                span(class="sr-only") Toggle navigation
                                span(class="icon-bar")
                                span(class="icon-bar")
                                span(class="icon-bar")
                            a(class="navbar-brand",href="#") Home
                        div(class="collapse navbar-collapse",id="bs-example-navbar-collapse-1")
                            ul(class="nav navbar-nav")
                                li(class="active")
                                    a(href="#") Problem
                                li(class="active")
                                    a(href="#") Status
                                li(class="active")
                                    a(href="#") Contest
                            form(class="navbar-form navbar-left",role="search")
                                div(class="form-group")
                                    input(type="text",class="form-control")
                                button(type="submit",class="btn btn-default") Submit
                            ul(class="nav navbar-nav navbar-right")
                                li(class="active")
                                    //-这个NAME还需要进一步设计
                                    a(href="#") Name 
                                li(class="active")
                                    //-这个Update更新到个人主页
                                    a(href="#") Update
                    div(class="jumbotron well")
                        p 
                            h3 比赛列表
                            
            div(class="pull-left")
                ul(class="pagination")
                    li
                        a(href="#") &laquo;
                    -for(var i = 1;i<=5;i++) 
                        li
                            a(href="#") #{i}
                    li 
                        a(href="#") &raquo;
            div(class="pull-right",id="bs-example-navbar-collapse-1")
                form(class="navbar-form navbar-left",role="search")
                    div(class="form-group")
                        input(type="text",class="form-control")
                    button(type="submit",class="btn btn-default") 搜索比赛
            table(class = "table")
                thead
                    tr
                        th 序号
                        th 比赛名
                        th 开始时间
                        th 结束时间
                        th 类型
                        th 状态
                tbody
                        -for(var i=1;i<=7;i++)
                            tr(class = "active")
                                td 00#{i}
                                th 
                                    a(href="./contest.html") icpc#{i}
                                th 0000.00
                                th 0000.00
                                th type#{i}
                                th status#{i}
        